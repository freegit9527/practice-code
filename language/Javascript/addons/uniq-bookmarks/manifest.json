{
  "manifest_version": 2,
  "name": "uniq-bookmarks",
  "version": "1.0",
  "description": "uniq all of your bookmarks, even the same bookmarks in different folder",
  "icons": {"128": "icon.png"},
  "background_page": "bg.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "uniq all of your bookmarks"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}


